        >Al-Fatehah</quran:transliteration>
        <quran:hasAyat rdf:resource="#AlFatehah1"/>
        <rdfs:label xml:lang="id">Pembukaan</rdfs:label>
        <rdfs:label xml:lang="en">The Opening</rdfs:label>
        <rdfs:label xml:lang="ar">‏الفاتحة‎</rdfs:label>
      </quran:Sura>
    </quran:isAyatOf>
    <quran:isAyatOf>
